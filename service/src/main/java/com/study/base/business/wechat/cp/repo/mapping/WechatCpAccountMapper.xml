<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.xluobo.business.wechat.cp.repo.mapper.WechatCpAccountMapper">

    <select id="selectAllInUserCpAccount" resultType="cn.xluobo.business.wechat.cp.repo.model.WechatCpAccount">
        select * from wechat_cp_account where in_use='1'
    </select>

    <select id="selectByCropIdAgentIdIgnoreTenant" resultType="cn.xluobo.business.wechat.cp.repo.model.WechatCpAccount">
        select * from wechat_cp_account where in_use='1' and corp_id=#{corpId} and agent_id=#{agentId} limit 1
    </select>
</mapper>
